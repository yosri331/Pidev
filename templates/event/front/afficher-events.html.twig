{% extends "event/front/front.html.twig" %}
{% block body %}
{% for e in event %}
<div class="container-fluid pt-5">
    <div class="row px-xl-5 pb-3">
        <div class="col-lg-4 col-md-6 pb-1">
            <div class="cat-item d-flex flex-column border mb-4" style="padding: 30px;">
                <p class="text-right">Comments:{{ e.count() }}</p>
                <a href="{{path('participer',{'id':e.id})}}" class="text-left">Particper!</a>
                <a href="{{path('front-eventdisplay',{'id':e.id})}}" class="cat-img position-relative overflow-hidden mb-3">
                    <img  class="img-fluid" src="{{asset('assets/uploads/images/events/' ~ e.getImage())}}" alt="">
                </a>
                <h5 class="font-weight-semi-bold m-0">{{ e.nom }}</h5>
            </div>
        </div>
        
        
    </div>
</div>
    
{% endfor %}

{% endblock %}